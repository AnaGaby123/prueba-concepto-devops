{
  "name": "proquifa-dot-net",
  "version": "1.147.6",
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "crypto": false
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ng-serve": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-dev": "ng build --configuration dev",
    "build-prod": "ng build --configuration prod",
    "build-qa": "ng build --configuration qa",
    "build-uat": "ng build --configuration uat",
    "uat-debug": "ng serve --configuration uat-debug",
    "prod-debug-serve": "ng serve --configuration prod-debug",
    "qa-serve": "ng serve --configuration qa"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.3.0",
    "@angular/cdk": "~14.2.7",
    "@angular/common": "~14.3.0",
    "@angular/compiler": "~14.3.0",
    "@angular/core": "~14.3.0",
    "@angular/forms": "~14.3.0",
    "@angular/material": "~14.2.7",
    "@angular/material-moment-adapter": "~14.2.7",
    "@angular/platform-browser": "~14.3.0",
    "@angular/platform-browser-dynamic": "~14.3.0",
    "@angular/router": "~14.3.0",
    "@aspnet/signalr": "^1.0.27",
    "@googlemaps/js-api-loader": "~1.14.3",
    "@iharbeck/ngx-virtual-scroller": "~14.0.4",
    "@ng-idle/core": "~12.0.4",
    "@ng-idle/keepalive": "~12.0.4",
    "@ngrx/effects": "~14.3.3",
    "@ngrx/router-store": "~14.3.3",
    "@ngrx/store": "~14.3.3",
    "@ngrx/store-devtools": "~14.3.3",
    "@ngx-translate/core": "~14.0.0",
    "@ngx-translate/http-loader": "~7.0.0",
    "@zerops/ngrx-store-logger": "~0.4.0",
    "accounting": "~0.4.1",
    "angular-user-idle": "~3.0.1",
    "animejs": "~3.2.1",
    "blob-to-sha256": "^0.0.1",
    "chart.js": "~3.9.1",
    "chartjs-plugin-datalabels": "~2.1.0",
    "lodash-es": "~4.17.21",
    "lottie-web": "~5.12.0",
    "moment": "~2.29.4",
    "ng-modal-lib-sab": "^14.0.0",
    "ng-swagger-gen": "~2.3.0",
    "ng2-dragula": "~2.1.1",
    "ngx-logger": "~5.0.12",
    "rxjs": "~7.5.0",
    "shelljs": "~0.8.5",
    "signalr-asp-net": "^1.0.3",
    "tslib": "~2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "~14.1.0",
    "@angular-devkit/build-angular": "~14.2.13",
    "@angular-devkit/core": "~14.2.13",
    "@angular-devkit/schematics": "~14.2.13",
    "@angular-eslint/builder": "~14.4.0",
    "@angular-eslint/eslint-plugin": "~14.4.0",
    "@angular-eslint/eslint-plugin-template": "~14.4.0",
    "@angular-eslint/schematics": "~14.4.0",
    "@angular-eslint/template-parser": "~14.4.0",
    "@angular/cli": "~14.2.13",
    "@angular/compiler-cli": "~14.3.0",
    "@angular/language-service": "~14.3.0",
    "@types/accounting": "~0.4.1",
    "@types/chart.js": "~2.9.37",
    "@types/google.maps": "~3.49.2",
    "@types/jest": "~26.0.24",
    "@types/lodash-es": "4.17.9",
    "@types/node": "~12.11.1",
    "@typescript-eslint/eslint-plugin": "~5.3.1",
    "@typescript-eslint/parser": "~5.3.1",
    "basic-ftp": "~5.0.3",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-prettier": "~4.2.1",
    "fs": "~0.0.1-security",
    "jest": "~28.1.3",
    "jest-preset-angular": "~12.2.3",
    "ngx-translate-testing": "^6.1.0",
    "node-fetch": "~2.6.9",
    "path": "~0.12.7",
    "prettier": "~2.0.5",
    "ts-node": "~9.1.1",
    "typescript": "~4.8.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-config/setup-jest.ts"
    ],
    "globalSetup": "jest-preset-angular/global-setup",
    "coveragePathIgnorePatterns": [
      "/jest-config/",
      "/node_modules/",
      "/.angular/",
      "/.idea/",
      "/.gradle/",
      "/projects/"
    ],
    "testMatch": [
      "**/+(*.)+(spec|test).+(ts)?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.module.ts",
      "!src/main.ts",
      "!src/polyfills.ts",
      "!src/environments/*.ts",
      "projects/simple-store/src/lib/**/*.ts",
      "!projects/simple-store/src/lib/**/*.module.ts",
      "!projects/simple-store/src/lib/index.ts",
      "!<rootDir>/jest-config/setup-jest.ts",
      "!<rootDir>/jest-config/jest-global-mocks.ts"
    ],
    "coverageReporters": [
      "html",
      "text"
    ],
    "moduleNameMapper": {
      "@appComponents/(.*)": "<rootDir>/src/app/components/$1",
      "@appCore/(.*)": "<rootDir>/src/app/core/$1",
      "@appGuards/(.*)": "<rootDir>/src/app/guards/$1",
      "@appInterfaces/(.*)": "<rootDir>/src/app/interfaces/$1",
      "@appModels/(.*)": "<rootDir>/src/app/models/$1",
      "@appPipes/(.*)": "<rootDir>/src/app/pipes/$1",
      "@appServices/(.*)": "<rootDir>/src/app/services/$1",
      "@appActions/(.*)": "<rootDir>/src/app/store/actions/$1",
      "@appSelectors/(.*)": "<rootDir>/src/app/store/selectors/$1",
      "@appReducers/(.*)": "<rootDir>/src/app/store/reducers/$1",
      "@appEffects/(.*)": "<rootDir>/src/app/store/effects/$1",
      "@appUtil/(.*)": "<rootDir>/src/app/util/$1",
      "@appHelpers/(.*)": "<rootDir>/src/app/helpers/$1",
      "@env/(.*)": "<rootDir>/src/environments/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@angular|@ngneat/spectator|array-move|lodash-es|@ngx-translate/core|ngx-translate-testing|@ngrx/store|@ngrx/router-store|@zerops/ngrx-store-logger|ngx-logger)"
    ]
  }
}
